{
    "name": "mon-theme-aca",
    "version": "1.0.0",
    "description": "Un thème WordPress modulable avec des blocs personnalisés",
    "main": "index.js",
    "scripts": {
        "build": "wp-scripts build blocks/stats-cards/index.js --output-path=blocks/stats-cards/build && wp-scripts build blocks/hero-slider/index.js --output-path=blocks/hero-slider/build && wp-scripts build blocks/recent-news/index.js --output-path=blocks/recent-news/build && wp-scripts build blocks/nos-missions/index.js --output-path=blocks/nos-missions/build && wp-scripts build blocks/events/index.js --output-path=blocks/events/build && wp-scripts build blocks/testimonials/index.js --output-path=blocks/testimonials/build && wp-scripts build blocks/partners/index.js --output-path=blocks/partners/build && wp-scripts build blocks/featured-post/index.js --output-path=blocks/featured-post/build && wp-scripts build blocks/filtered-posts/index.js --output-path=blocks/filtered-posts/build && wp-scripts build blocks/timeline/index.js --output-path=blocks/timeline/build",
        "start": "wp-scripts start blocks/stats-cards/index.js --output-path=blocks/stats-cards/build",
        "dev": "wp-scripts start blocks/stats-cards/index.js --output-path=blocks/stats-cards/build",
        "build:hero-slider": "wp-scripts build blocks/hero-slider/index.js --output-path=blocks/hero-slider/build",
        "start:hero-slider": "wp-scripts start blocks/hero-slider/index.js --output-path=blocks/hero-slider/build",
        "build:recent-news": "wp-scripts build blocks/recent-news/index.js --output-path=blocks/recent-news/build",
        "start:recent-news": "wp-scripts start blocks/recent-news/index.js --output-path=blocks/recent-news/build",
        "build:nos-missions": "wp-scripts build blocks/nos-missions/index.js --output-path=blocks/nos-missions/build",
        "start:nos-missions": "wp-scripts start blocks/nos-missions/index.js --output-path=blocks/nos-missions/build",
        "build:events": "wp-scripts build blocks/events/index.js --output-path=blocks/events/build",
        "start:events": "wp-scripts start blocks/events/index.js --output-path=blocks/events/build",
        "build:testimonials": "wp-scripts build blocks/testimonials/index.js --output-path=blocks/testimonials/build",
        "start:testimonials": "wp-scripts start blocks/testimonials/index.js --output-path=blocks/testimonials/build",
        "build:partners": "wp-scripts build blocks/partners/index.js --output-path=blocks/partners/build",
        "start:partners": "wp-scripts start blocks/partners/index.js --output-path=blocks/partners/build",
        "build:featured-post": "wp-scripts build blocks/featured-post/index.js --output-path=blocks/featured-post/build",
        "start:featured-post": "wp-scripts start blocks/featured-post/index.js --output-path=blocks/featured-post/build",
        "build:filtered-posts": "wp-scripts build blocks/filtered-posts/index.js --output-path=blocks/filtered-posts/build",
        "start:filtered-posts": "wp-scripts start blocks/filtered-posts/index.js --output-path=blocks/filtered-posts/build",
        "build:timeline": "wp-scripts build blocks/timeline/index.js --output-path=blocks/timeline/build",
        "start:timeline": "wp-scripts start blocks/timeline/index.js --output-path=blocks/timeline/build",
        "build:all": "npm run build"
    },
    "devDependencies": {
        "@wordpress/scripts": "^26.0.0"
    },
    "dependencies": {
        "@wordpress/block-editor": "^12.0.0",
        "@wordpress/blocks": "^12.0.0",
        "@wordpress/components": "^25.0.0",
        "@wordpress/core-data": "^6.0.0",
        "@wordpress/data": "^9.0.0",
        "@wordpress/element": "^5.0.0",
        "@wordpress/i18n": "^4.0.0"
    }
}